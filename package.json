{
  "name": "typingmind-chatbot-platform",
  "version": "1.0.0",
  "description": "Multi-tenant embeddable chatbot platform powered by TypingMind API",
  "main": "worker/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "dev:widget": "npm run build:widget && npx http-server widget -p 8080",
    "build": "npm run build:widget",
    "build:widget": "node widget/build.js",
    "deploy": "wrangler deploy",
    "deploy:widget": "npm run build:widget && node scripts/deploy-widget.js",
    "test": "jest",
    "lint": "eslint .",
    "setup:kv": "node scripts/setup-kv.js",
    "config:push": "node scripts/push-config.js",
    "serve:demo": "npx http-server widget -p 8080 -o demo.html"
  },
  "keywords": ["chatbot", "typingmind", "cloudflare-workers", "widget"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "wrangler": "^3.0.0",
    "uglify-js": "^3.17.4",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "itty-router": "^4.0.0"
  }
}